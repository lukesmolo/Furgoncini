<!DOCTYPE html>
<html>
	<head>
		<meta charset='UTF-8' />

		<link rel="stylesheet" href="/css/print.css" media="print">
		<link href="/bootstrap-3.3.4-dist/css/bootstrap.min.css" rel="stylesheet" media="all">




		<title><%= @page_title %></title>
	</head>
	<body>
		<% n = 0 %>
		<div id="print_main" class="col-md-12">
			<div id="0" class="col-md-4 pull-left">
				<% @paths0.each do |x| %>
					<table id="print_table_<%= n %>" class="table print_table table-striped">
						<thead >
							<th><%= x["name"] %></th><th><%= x["time"] %></th></tr>
						</thead>
						<tbody id="">
							<% x["shops"].each do |y| %>
								<tr><td><%= y["city"] %></td><td><%= y["name"] %></td></tr>
							<% end %>
						</tbody>
					</table>
					<% n += 1 %>
				<% end %>
			</div>
			<div id="1" class=" col-md-4">
				<% @paths1.each do |x| %>
					<table id="print_table_<%= n %>" class="table print_table table-striped">
						<thead >
							<th><%= x["name"] %></th><th><%= x["time"] %></th></tr>
						</thead>
						<tbody id="">
							<% x["shops"].each do |y| %>
								<tr><td><%= y["city"] %></td><td><%= y["name"] %></td></tr>
							<% end %>
						</tbody>
					</table>
					<% n += 1 %>
				<% end %>
			</div>
			<div id="2" class="col-md-4 pull-right">
				<% @paths2.each do |x| %>
					<table id="print_table_<%= n %>" class="table print_table table-striped">
						<thead >
							<th><%= x["name"] %></th><th><%= x["time"] %></th></tr>
						</thead>
						<tbody id="">
							<% x["shops"].each do |y| %>
								<tr><td><%= y["city"] %></td><td><%= y["name"] %></td></tr>
							<% end %>
						</tbody>
					</table>
					<% n += 1 %>
				<% end %>

			</div>
		</div>
	</body>
</html>
